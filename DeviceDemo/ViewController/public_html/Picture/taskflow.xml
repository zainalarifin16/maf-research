<?xml version="1.0" encoding="ISO-8859-1" ?>
<adfc-mobile-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.0">
  <task-flow-definition id="taskflow">
    <default-activity>Picture</default-activity>
    <view id="Picture">
      <page>Picture/Picture.amx</page>
    </view>
    <view id="Result">
      <page>Picture/Result.amx</page>
    </view>
    <control-flow-rule id="__1">
      <from-activity-id>Picture</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>result</from-outcome>
        <to-activity-id>Result</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
  </task-flow-definition>
</adfc-mobile-config>
