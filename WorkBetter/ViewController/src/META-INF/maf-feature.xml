<?xml version="1.0" encoding="UTF-8" ?>
<adfmf:features xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:adfmf="http://xmlns.oracle.com/adf/mf">
    <adfmf:feature id="wb.Login" name="Login">
        <adfmf:content id="wb.Login.SmallPhone">
            <adfmf:constraints>
                <adfmf:constraint property="hardware.screen.diagonalSize" operator="less" value="4" id="c1"/>
            </adfmf:constraints>
            <adfmf:amx file="wb.Login/login.amx">
                <adfmf:includes>
                    <adfmf:include type="StyleSheet" file="wb.Login/login.css" id="i2"/>
                    <adfmf:include type="StyleSheet" file="wb.Login/smallPhone.css" id="i5"/>
                </adfmf:includes>
            </adfmf:amx>
        </adfmf:content>
        <adfmf:content id="wb.Login.LargePhone">
            <adfmf:constraints>
                <adfmf:constraint property="hardware.screen.diagonalSize" operator="less" value="6" id="c2"/>
            </adfmf:constraints>
            <adfmf:amx file="wb.Login/login.amx">
                <adfmf:includes>
                    <adfmf:include type="StyleSheet" file="wb.Login/login.css" id="i3"/>
                    <adfmf:include type="StyleSheet" file="wb.Login/largePhone.css" id="i6"/>
                </adfmf:includes>
            </adfmf:amx>
        </adfmf:content>
        <adfmf:content id="wb.Login.Tablet">
            <adfmf:amx file="wb.Login/login.amx">
                <adfmf:includes>
                    <adfmf:include type="StyleSheet" file="wb.Login/login.css" id="i1"/>
                    <adfmf:include type="StyleSheet" file="wb.Login/tablet.css" id="i4"/>
                </adfmf:includes>
            </adfmf:amx>
        </adfmf:content>
        <adfmf:properties id="p9">
            <adfmf:property name="iOSWebView" value="modern" id="p10"/>
        </adfmf:properties>
    </adfmf:feature>
    <adfmf:feature id="wb.Springboard" name="Springboard">
        <adfmf:content id="wb.Springboard.1">
            <adfmf:amx file="wb.Springboard/springboard.amx">
                <adfmf:includes/>
            </adfmf:amx>
        </adfmf:content>
        <adfmf:properties id="p1">
            <adfmf:property name="iOSWebView" value="modern" id="p2"/>
        </adfmf:properties>
    </adfmf:feature>
    <adfmf:feature id="wb.Directory" name="Directory">
        <adfmf:content id="wb.Directory.1">
            <adfmf:amx file="wb.Directory/Directory_taskflow.xml#taskflow"/>
        </adfmf:content>
        <adfmf:properties id="p11">
            <adfmf:property name="iOSWebView" value="modern" id="p12"/>
        </adfmf:properties>
    </adfmf:feature>
    <adfmf:feature id="wb.Dashboard" name="Dashboard">
        <adfmf:content id="wb.Dashboard.1">
            <adfmf:amx file="wb.Dashboard/Dashboard_taskflow.xml#taskflow">
                <adfmf:includes/>
            </adfmf:amx>
        </adfmf:content>
        <adfmf:properties id="p7">
            <adfmf:property name="iOSWebView" value="modern" id="p8"/>
        </adfmf:properties>
    </adfmf:feature>
    <adfmf:feature id="wb.MyProfile" name="MyProfile">
        <adfmf:content id="wb.MyProfile.1">
            <adfmf:amx file="wb.Profile/MyProfileTF.xml#ProfileTF"/>
        </adfmf:content>
        <adfmf:properties id="p3">
            <adfmf:property name="iOSWebView" value="modern" id="p4"/>
        </adfmf:properties>
    </adfmf:feature>
    <adfmf:feature id="wb.MyOrg" name="MyOrg">
        <adfmf:content id="wb.MyOrg.1">
            <adfmf:amx file="wb.MyOrg/MyOrg_Launcher.xml#MyOrg_Launcher">
