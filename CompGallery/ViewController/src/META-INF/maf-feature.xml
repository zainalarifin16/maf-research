<?xml version="1.0" encoding="UTF-8" ?>
<adfmf:features xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:adfmf="http://xmlns.oracle.com/adf/mf">
  <adfmf:feature id="About" name="About">
    <adfmf:content id="About.1">
      <adfmf:amx file="About/task-flow.xml#task-flow">
        <adfmf:includes>
          <adfmf:include type="StyleSheet" file="css/custom.css" id="iAbout"/>
        </adfmf:includes>
      </adfmf:amx>
    </adfmf:content>
    <adfmf:properties id="p3">
      <adfmf:property name="iOSWebView" value="default" id="p4"/>
    </adfmf:properties>
  </adfmf:feature>
  <adfmf:feature id="Gallery" name="Components">
    <adfmf:content id="Gallery.1">
      <adfmf:amx file="Gallery/task-flow.xml#task-flow">
        <adfmf:includes>
          <adfmf:include type="StyleSheet" file="css/custom.css" id="i1"/>
          <adfmf:include type="JavaScript" file="js/custom.js" id="i2"/>
          <adfmf:include type="JavaScript" file="js/properties.js" id="i3"/>
        </adfmf:includes>
      </adfmf:amx>
    </adfmf:content>
    <adfmf:properties id="p1">
      <adfmf:property name="iOSWebView" value="default" id="p2"/>
    </adfmf:properties>
  </adfmf:feature>
  <adfmf:feature id="Springboard" name="Springboard">
    <adfmf:content id="Springboard.1">
      <adfmf:amx file="Springboard/springboard-task-flow.xml#springboard-task-flow"/>
    </adfmf:content>
    <adfmf:properties id="p5">
      <adfmf:property name="iOSWebView" value="default" id="p6"/>
    </adfmf:properties>
  </adfmf:feature>
</adfmf:features>
