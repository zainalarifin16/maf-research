          <dvtm:chartDataItem group="10/5/2015" value="10"/>
          <dvtm:chartDataItem group="10/6/2015" value="14"/>
        </amx:facet>
      </dvtm:areaChart>
      <amx:panelGroupLayout id="pglOptionsPanel" scrollPolicy="scroll">
        <amx:outputText id="otDesc" inlineStyle="font-size: 12px; padding: 16px; display: block;"
                        value="When chart with time axis has data with irregular intervals, we display the data at the actual time locations by default (timeAxisType='enabled'). However, there are cases where the time intervals are irregular, but the app wants to display the groups at a regular distance. This is very common in stock charts where the after-hour or the weekend data don't exist, but the app doesn't want the chart to display the gaps."/>
        <amx:panelGroupLayout id="pgl1" layout="horizontal">
          <amx:selectOneButton label="timeAxisType = " id="sob1" value="#{pageFlowScope.timeAxisType}"
                               inlineStyle="width: 350px">
            <amx:selectItem id="si1" label="enabled" value="enabled"/>
            <amx:selectItem id="si2" label="skipGaps" value="skipGaps"/>
          </amx:selectOneButton>
        </amx:panelGroupLayout>
        <amx:selectOneButton label="Chart Type:" id="sob2" value="#{pageFlowScope.chartType}">
          <amx:selectItem id="si11" label="Bar Chart" value="barChart"/>
          <amx:selectItem id="si12" label="Line Chart" value="lineChart"/>
          <amx:selectItem id="si13" label="Area Chart" value="areaChart"/>
        </amx:selectOneButton>
      </amx:panelGroupLayout>
    </amx:flexLayout>
  </amx:panelPage>
</amx:view>
