      <page>/markerShapeAttribute.amx</page>
    </view>
    <view id="chartAxisLogaritmicScale">
      <page>/chartAxisLogaritmicScale.amx</page>
    </view>
    <control-flow-rule id="__1">
      <from-activity-id>dvtFeatures</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>return</from-outcome>
        <to-activity-id>return</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__5">
